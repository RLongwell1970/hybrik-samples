{
  "definitions": {
    "profile_name": "analyze_general_properties",
		"type": "features",
    "category": "analysis",
		"source_filename": "tears_of_steel_1080p.mov",

		"source_path": "s3://hybrik-examples/public/sources",
		"source": "{{source_path}}/{{source_filename}}"
  },
  "name": "{{profile_name}}:  {{source}}",
  "payload": {
    "elements": [
      {
        "uid": "source_file",
        "kind": "source",
        "payload": {
          "kind": "asset_url",
          "payload": {
            "storage_provider": "s3",
            "url": "{{source}}"
          }
        }
      },
      {
        "uid": "analyze_task",
        "kind": "analyze",
        "payload": {
          "general_properties": {
            "enabled": true
          }
        }
      }
    ],
    "connections": [
      {
        "from": [
          {
            "element": "source_file"
          }
        ],
        "to": {
          "success": [
            {
              "element": "analyze_task"
            }
          ]
        }
      }
    ]
  }
}