{
    "definitions": {
        "descriptor": "delete_file",
        "delete_file": "tears_of_steel_720p.mov",
        "destination": "s3://hybrik-examples/public/output/feature_examples/data_movement/copy_file/{{delete_file}}"
    },
    "name": "{{descriptor}}: {{destination}}",
    "payload": {
        "elements": [
            {
                "uid": "source_file",
                "kind": "source",
                "payload": {
                    "kind": "asset_url",
                    "payload": {
                        "storage_provider": "s3",
                        "url": "{{destination}}"
                    }
                }
            },
            {
                "uid": "delete_task",
                "kind": "delete_asset",
                "payload": {
                    "asset_selector": "workflow_document"
                }
            }
        ],
        "connections": [
            {
                "from": [
                    {
                        "element": "source_file"
                    }
                ],
                "to": {
                    "success": [
                        {
                            "element": "delete_task"
                        }
                    ]
                }
            }
        ]
    }
}